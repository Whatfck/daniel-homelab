# Caddyfile

lab.tailbb818c.ts.net {
    # Reverse proxy for arcane at /arcane/
    route /arcane/* {
        uri strip_prefix /arcane
        reverse_proxy arcane:3552
    }

    # Reverse proxy for dashy at root
    route /* {
        reverse_proxy dashy:8080
    }
}

# Default catch-all
* {
    respond "Hello from Caddy!" 200
}